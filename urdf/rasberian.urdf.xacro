<?xml version="1.0" ?>
<robot name="rasberian" xmlns:xacro="http://ros.org/wiki/xacro">


    <link name="base_link">

        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.33"/>
            <inertia ixx="4.85375e-4" iyy="3.884375e-4" izz="7.74e-4" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <collision>

            <geometry>
                <box size="0.125 0.11 0.045"/>
            </geometry>

        </collision>

        <visual>
            <origin rpy="0 3.14 -1.57" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rasberian/model/rasberian_body.dae" scale="0.001 0.001 0.001">
                    <!-- <uri>package://rasberian/model/rasberian_body.dae</uri> -->
                </mesh>
            </geometry>

            <material name="silver">
                <!-- <color rgba="0.75 0.75 0.75 1"/> -->
                <!-- <script>
                    <uri>/home/ahal/catkin_ws/src/rasberian/model/materials/scripts/marker.material</uri>
                    <uri>/home/ahal/catkin_ws/src/rasberian/model/materials/texture/model.jpg</uri>
                    <name>rasberian/body</name>
                </script> -->
                <texture filename="/home/ahal/catkin_ws/src/rasberian/model/materials/scripts/model.jpg" />
            </material>
        </visual>

    </link>

    <link name="back_right_wheel">
         <inertial> 
            <mass value="0.03"/>
            <inertia ixx="6.67e-6" iyy="6.67e-6" izz="1.89e-6" ixy="0" ixz="0" iyz="0"/>
        </inertial>
        <collision>
            <geometry>
                <cylinder length="0.02" radius="0.0275"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.02" radius="0.0275"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
    </link>


    <joint name="back_right_wheel_joint" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="back_right_wheel"/>
        <origin rpy="-1.5708 0 0" xyz="-0.0225 -0.065 -0.003"/>
    </joint>


    <link name="back_left_wheel">
         <inertial> 
            <mass value="0.03"/>
            <inertia ixx="6.67e-6" iyy="6.67e-6" izz="1.89e-6" ixy="0" ixz="0" iyz="0"/>
        </inertial>
        <collision>
            <geometry>
                <cylinder length="0.02" radius="0.0275"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.02" radius="0.0275"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>
    </link>

    <joint name="back_left_wheel_joint" type="continuous">
        <axis xyz="0 0 1"/>
        <parent link="base_link"/>
        <child link="back_left_wheel"/>
        <origin rpy="-1.5708 0 0" xyz="-0.0225 0.065 -0.003"/>
    </joint> 



    <link name="caster">
    <visual>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0 "/>
      <geometry>
        <sphere radius="0.00625"/>
      </geometry>
        <material name="black">
             <color rgba="0 0 0 1"/>
        </material>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.00625"/>
      </geometry>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0 "/>
    </collision>
    <inertial>
      <mass value="0.005"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0000025" ixy="0.0" ixz="0.0" iyy="0.0000025" iyz="0.0" izz="0.0000025"/>
    </inertial>
    
  </link>

  <!-- <joint name="caster_joint" type="fixed">
    <parent link="base_link"/>
    <child link="caster"/>
    <origin rpy="0 0 0" xyz="0.03 0.0 -0.02825"/>
  </joint> -->
    <link name="caster_support">

        <inertial>
            <mass value="0.01"/>
            <inertia ixx="4.85375e-4" iyy="3.884375e-4" izz="7.74e-4" ixy="0" ixz="0" iyz="0"/>
        </inertial>

        <collision>

            <geometry>
                <box size="0.01 0.01 0.04"/>
            </geometry>

        </collision>

        <visual>
            <geometry>
                <box size="0.01 0.01 0.04"/>
            </geometry>

            <material name="silver">
                <color rgba="0.75 0.75 0.75 1"/>
            </material>
        </visual>
    </link>

    <joint name="caster_support_joint" type="fixed">
        <parent link="base_link"/>
        <child link="caster_support"/>
        <origin rpy="0 0 0" xyz="0.03 0.0 -0.002"/>
        <!-- <axis xyz="0 0 1"/> -->
    </joint>

    <joint name="caster_joint" type="continuous">
        <parent link="caster_support"/>
        <child link="caster"/>
        <origin rpy="0 0 0" xyz="0.0 0.0 -0.02525"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- gazebo -->
    <gazebo reference="base_link">
        <material>Gazebo/model</material>
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
    </gazebo>
    <gazebo reference="back_left_wheel">
        <material>Gazebo/Black</material>
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
    </gazebo>
    <gazebo reference="back_right_wheel">
        <material>Gazebo/Black</material>
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
    </gazebo>
    <gazebo reference="front_left_wheel">
        <material>Gazebo/Black</material>
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
    </gazebo>
    <gazebo reference="front_right_wheel">
        <material>Gazebo/Black</material>
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
    </gazebo>
    <gazebo reference="caster">
        <material>Gazebo/Black</material>
        <mu1>0.1</mu1>
        <mu2>0.1</mu2>
    </gazebo>
    
</robot>
